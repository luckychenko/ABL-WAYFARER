{
  "name": "abl-wayfarer",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "prestart": "npm run dbmaketables",
    "start": "node  ./dist/bin/www",
    "prestart:dev": "npm run dbmaketables",
    "start:dev": "cross-env NODE_ENV=development nodemon --watch .env --watch src --exec babel-node ./src/bin/www --source-maps",
    "dev": "cross-env NODE_ENV=development  nodemon --watch routes,model,services --exec babel-node -- ./src/bin/www --source-maps",
    "serve": "cross-env NODE_ENV=production  node dist/bin/www",
    "build": "rimraf dist && babel src --out-dir dist",
    "dbmaketables": "node ./src/services/database createTables",
    "dbdroptables": "node ./src/services/database dropTables",
    "pretest": "cross-env NODE_ENV=test npm run dbmaketables",
    "posttest": "cross-env NODE_ENV=test npm run dbdroptables",
    "test": "cross-env NODE_ENV=test nyc mocha --exit  --reporter spec --recursive --require @babel/register \"src/controllers/**/*.test.js\" ",
    "coverage": "nyc report --reporter=text-lcov | coveralls"
  },
  "keywords": [
    "wayfarer",
    "javascript",
    "node",
    "express",
    "es6",
    "api"
  ],
  "author": "Lucky Ogidan <luckychenko@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@babel/runtime": "^7.5.4",
    "@babel/runtime-corejs2": "^7.5.4",
    "acorn": "^6.1.1",
    "bcryptjs": "^2.4.3",
    "chai": "^4.2.0",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^8.0.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "express": "~4.16.1",
    "helmet": "^3.18.0",
    "http-errors": "~1.6.3",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.14",
    "mocha": "^6.1.4",
    "morgan": "~1.9.1",
    "pg": "^7.11.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.0",
    "@babel/core": "^7.5.4",
    "@babel/node": "^7.5.0",
    "@babel/plugin-transform-runtime": "^7.5.0",
    "@babel/preset-env": "^7.5.4",
    "@babel/register": "^7.4.4",
    "coveralls": "^3.0.5",
    "cross-env": "^5.2.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.2.0",
    "eslint-plugin-import": "^2.18.0",
    "make-runnable": "^1.3.6",
    "mocha-lcov-reporter": "^1.3.0",
    "nodemon": "^1.19.1",
    "nyc": "^14.1.1",
    "rimraf": "^2.6.3",
    "supertest": "^4.0.2"
  }
}
